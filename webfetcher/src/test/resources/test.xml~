<!--
	Image search engines
-->
<page name="www.altavista.com/video">
	<expression type="list" 	name="result-hrefs" 			value="//tr[@class='resultGroup']//a[@class='thumbnail']/@href"/>
	<expression type="list" 	name="result-thumbnails" 		value="//tr[@class='resultGroup']//img[@class='thumbnail']/@src"/>
	<expression type="list" 	name="result-descriptions" 		value="//tr[@class='resultGroup']//b"/>
	<expression type="list" 	name="result-file-filesize" 	value="//tr[@class='resultGroup']//span[@class='resSize']/text()[1]"/>
	<expression type="list" 	name="result-file-dimentions" 	value="//tr[@class='resultGroup']//span[@class='resSize']/text()[1]"/>
	<expression type="list" 	name="result-information-href" 	value="//span[@class='resSize']/a/@href"/>
	<expression type="string" 	name="numresults" 				value="//td[@colspan='4']/div[@class='xs']/b[@class='lbl']"/>
</page>

