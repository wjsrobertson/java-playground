</page>
<page name="google.com">
	<expression type="list" 	name="result-hrefs" 		value="//div[@class='g']/h2[@class='r']/a/@href"/>
	<expression type="list" 	name="result-anchortexts" 	value="//div[@class='g']/h2[@class='r']/a"/>
	<expression type="list" 	name="result-descriptions" 	value="//div[@class='g']/table/tr/td/div[@class='std']/descendant::text()[1]"/>
	<expression type="string" 	name="numresults" 			value="//table[@class='t bt']/tr/td/b[3]"/>
</page>
<page name="uk.ask.com">
	<expression type="list" 	name="result-hrefs" 		value="//div[@id='teoma-results']/div//table/tr/td[2]/a[@class='L4']/@href"/>
